<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane xmlns="http://javafx.com/javafx" xmlns:fx="http://javafx.com/fxml" fx:controller="org.assign2.WelcomeController" prefHeight="400.0" prefWidth="600.0">
    <Label text="Welcome to linking game"/>

    <Button text="Row" fx:id="rowButton" AnchorPane.leftAnchor="10" AnchorPane.topAnchor="50"/>

    <Button text="Column" fx:id="colButton" AnchorPane.rightAnchor="10" AnchorPane.topAnchor="50"/>

    <Button text="Start!" fx:id="startButton" AnchorPane.bottomAnchor="10" onAction="#handleStart"/>
</AnchorPane>
